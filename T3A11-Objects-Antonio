<script>

// set the object
function myStudent(myAge, myName, myImg){
    this.myStudentAge = myAge
    this.myStudentName = myName
    this.myStudentImage = myImg
    this.myInfo = myStudentInfo
    }
    
function myStudentInfo(){
    document.getElementById('myDiv1').innerHTML = ' My name is ' +
        this.myStudentName + ' I am ' + this.myStudentAge +
            ' years old. Here is my picture <img width=77 src='+this.myStudentImage+'><br>'
    }
    

</script>

<input type=button value="Fred Object" onclick="{
    myFred = new myStudent(100, 'Fred', 'https://www.rover.com/blog/wp-content/uploads/2015/05/dog-candy-junk-food-599x340.jpg')
    myFred.myInfo()
}">

