<script>

// set the object
function myStudent(myAge, myName, myImg){
   this.myStudentAge = myAge
   this.myStudentName = myName
   this.myStudentImage = myImg
   this.myInfo = myStudentInfo
}

function myStudentInfo(){
   document.getElementById('myDiv1').innerHTML += ' My name is ' + 
       this.myStudentName + ' I am ' + this.myStudentAge +
       ' years old. Here is my picture <br><img width=70 src =' + this.myStudentImage +
       '><br><br>'
}

</script>



<input type=button value="Fred Object" onclick="{
    myFred = new myStudent(100, 'Fred', 'https://www.rover.com/blog/wp-content/uploads/2015/05/dog-candy-junk-food-599x340.jpg')
    myFred.myInfo()
}">

<input type=button value="Jim Object" onclick="{
    myJim = new myStudent(200, 'Jim', 'https://cdn.pixabay.com/photo/2015/09/06/20/31/frog-927765_960_720.jpg')
    myJim.myInfo()
}">

<input type=button value="Bob Object" onclick="{
    myBob = new myStudent(300, 'Bob', 'http://static2.businessinsider.com/image/5654150584307663008b4ed8/this-tech-recruiter-owns-the-most-hipster-cat-on-instagram.jpg')
    myBob.myInfo()
}">

